{
  "manifest_version": 2,
  "name": "Sidebar Data",
  "version": "1.0",
  "description": "Displays data from the current page in the sidebar popup.",
  "permissions": [
    "activeTab",
    "http://localhost:8000/*"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {}
  },
  "icons": {}
}
